    "react-redux": "7.1.1",
    "react-router": "5.0.1",
    "react-router-redux": "4.0.8",
    "react-tabs": "^3.0.0",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "sprintf-js": "^1.0.3",
    "toastr": "2.1.4"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
